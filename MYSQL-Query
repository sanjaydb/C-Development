show tables ;
Fields -> 	(org_name,ciso_name,ciso_email,ciso_phone,entry_date)

create table ClientInfo ( 
	c_id INT(10) NOT NULL AUTO_INCREMENT,
	org_name  CHAR(50),
	ciso_name CHAR(50),
	ciso_email CHAR(50),
	ciso_phone CHAR(50),
	entry_date DATE ,
	PRIMARY KEY (c_id)
);

