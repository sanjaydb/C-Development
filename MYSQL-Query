Nessus File Import 
select ClientID,ActivityID,ActivityName,Plugin_ID,Risk,Host,Name, Protocol from Ativity ;

create table Ativity (
ClientID INT(10) not null, 
ActivityID INT(10) not null,
ActivityName CHAR(50),
Plugin_ID VARCHAR(10),	
CVE VARCHAR(15),	
CVSSv2BS VARCHAR(15),	
Risk VARCHAR(10),	
Host VARCHAR(15),	
Protocol VARCHAR(10),	
Port VARCHAR(8),	
Name VARCHAR(200),	
Synopsis VARCHAR(200),	
Description VARCHAR(300),	
Solution VARCHAR(300),	
SeeAlso VARCHAR(100),	
PluginOutput VARCHAR(100),	
STIGSeverity VARCHAR(15),	
CVSSv3BS VARCHAR(20),	
CVSSv2TS VARCHAR(20),	
CVSSv3TS VARCHAR(20),	
RiskFactor VARCHAR(11),	
BID VARCHAR(200),	
XREF VARCHAR(300),	
MSKB VARCHAR(100),	
PluginPubDate VARCHAR(20),	
PluginModDate VARCHAR(20),	
Metasploit VARCHAR(10),	
Core VARCHAR(10),  
Impact VARCHAR(10),	
CANVAS VARCHAR(10)

);



