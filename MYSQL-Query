drop table AtivityMaster;
drop table ClientInfo ;

delete from AtivityMaster ;
delete from Clientinfo ;


create table AtivityMaster (
ClientID INT(10) not null, 
ActivityID INT(10) not null,
ActivityName CHAR(50),
Frequency smallint(2),
TaskPriority CHAR(1),
StartDate	DATE,
ScanCount smallint(2),
EndDate	DATE,
ScopIPs INT(5),
ScanStatus char(10),
Remarks VARCHAR(512),
ReportDate DATE,
ReportRef char(50),
ActivityStatus char(10),
CertIn CHAR(1),
ReportName char(50),
PoNumber char(50),
PRIMARY KEY (ActivityID)
);

