select * from ActivityMaster ;




/* --------------------------------------------------------------------------------------------------------------------- */
        
        
("xuv_id","xua_id","c_id","a_id","ip_id","Ativity_Name","freq","ativity_status","scaned_ip","critical","high","medium","low","info","extra_bit","extra_ip_start_date","assignee_to","v_id","vul_name","vul_fixed","vul_fixed_date");

create table ActivityData (
xuv_id INT(10) NOT NULL AUTO_INCREMENT,
xua_id INT(10) NOT NULL,
c_id INT(10) NOT NULL,
a_id INT(10) NOT NULL,
ip_id INT(10) NOT NULL,
Ativity_Name CHAR(5),
freq TINYINT,
ativity_status CHAR(4),
scaned_ip VARCHAR(15),
critical int(1),
high int(1),
medium int(1),
low int(1),
info int(1),
extra_bit bool,
extra_ip_start_date DATE,
assignee_to CHAR(15),
v_id int(9),
vul_name VARCHAR(5),
vul_fixed bool,
vul_fixed_date DATE,
PRIMARY KEY (xuv_id)
);

ALTER TABLE ActivityData AUTO_INCREMENT=5001;
