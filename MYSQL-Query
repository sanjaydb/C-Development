drop table ActivityMaster ;


create table ActivityMaster (
		xua_id  INT(10) NOT NULL AUTO_INCREMENT,
		c_id INT(10) NOT NULL,
		a_id INT(10) NOT NULL,
		Ativity_Name CHAR(5),
		freq TINYINT, 
		ativity_status CHAR(4),
		ativity_ips_done SMALLINT,
		scope_ip SMALLINT,
		crm_no INT,
		start_date DATE,
		end_date DATE,
		extra_ips_bit bool,
		extra_ips_howmuch SMALLINT,
		extra_ip_start_date DATE,
		assignee_to CHAR(15),
		PRIMARY KEY (xua_id)
);

ALTER TABLE ActivityMaster AUTO_INCREMENT=9001;

(xua_id,c_id,a_id,Ativity_Name,freq,ativity_status,ativity_ips_done,scope_ip,crm_no,start_date,end_date,extra_ips_bit,extra_ips_howmuch,extra_ip_start_date,assignee_to)

