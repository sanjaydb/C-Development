describe AtivityMaster ;

select * from AtivityMaster ;
describe AtivityMaster ;

select * from AtivityMaster WHERE startDate >= '2021-01-01' AND EndDate <= '2022-08-01' ;


select * from AtivityMaster WHERE startDate >= '2021-01-01' AND EndDate <= '2022-08-01' and ActivityName="VAPT";

select ActivityID, ActivityName,Frequency,ScanCount, ScopIPs from AtivityMaster WHERE StartDate >= '2021-01-01' AND EndDate <= '2022-08-01' is not NULL and ActivityName="VAPT";

select s.ClientID,s.OrganizationName,q.ActivityID, q.ActivityName,q.Frequency,q.ScanCount, q.ScopIPs from ClientInfo s Left Join AtivityMaster q on q.ClientID = s.ClientID; 


select * from AtivityMaster ;
select * from ClientInfo ;


select ActivityID, ActivityName,Frequency,ScanCount, ScopIPs from AtivityMaster WHERE StartDate >= '2021-01-01' AND EndDate <= '2022-08-01' is not NULL and ActivityName="VAPT";

select ClientID,ActivityID, ActivityName,Frequency,ScanCount, ScopIPs, DATE_FORMAT(startDate,'%d/%m/%Y') AS sDate , DATE_FORMAT(EndDate,'%d/%m/%Y') 
AS eDate from AtivityMaster;

select s.OrganizationName, q.ActivityName,q.Frequency,q.ScanCount, q.ScopIPs , DATE_FORMAT(q.startDate,'%d/%m/%Y') AS sDate ,DATE_FORMAT(q.EndDate,'%d/%m/%Y') 
AS eDate from ClientInfo s Left Join AtivityMaster q on  
q.ClientID = s.ClientID WHERE q.startDate >= '2022-01-21' and q.EndDate <= '2022-08-26' ;





