Working 
----------
select ClientInfo.OrganizationName , AtivityMaster.ActivityName from AtivityMaster join ClientInfo where AtivityMaster.Frequency="4" and 
AtivityMaster.ClientID="231205" and ClientInfo.ClientID="231205" ;

select  ActivityName , count(ActivityName) from AtivityMaster group by ActivityName ;

select ActivityName , DATE_FORMAT(startDate,'%d/%m/%Y') AS sDate ,DATE_FORMAT(EndDate,'%d/%m/%Y') AS eDate from AtivityMaster 
WHERE startDate >= '2022-01-21' and EndDate <= '2022-01-26' ;


select ClientID,ActivityID, ActivityName,Frequency,ScanCount, ScopIPs, DATE_FORMAT(startDate,'%d/%m/%Y') AS sDate , DATE_FORMAT(EndDate,'%d/%m/%Y') 
AS eDate from AtivityMaster;


